{
  "name": "cv-trace",
  "version": "0.0.1",
  "description": "A library for tracing images to SVG",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "image",
    "svg",
    "trace",
    "vectorize"
  ],
  "author": "yoclo",
  "license": "GPL-2.0-or-later",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/potrace": "^2.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "jimp": "^1.6.0",
    "potrace": "^2.1.8"
  }
}
